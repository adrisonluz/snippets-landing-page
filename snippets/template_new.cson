            						$('#aba_content').appendTo('.conceitual .content');
                        // FANCYBOX NAS IMAGENS //
            						setFancybox('.imgDetalhe', produto);

            						// MAPAS //
                        maps(mapsArray, '.unidades-rodape .unidade');

            						// EXCLUI BLOCOS INTEIROS //
