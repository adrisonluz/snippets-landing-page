'*':
    'template_novo':
        'prefix': 'tmpNew'
        'body': """
            <script>
            /*
            *    Função @widthContent
            *    @description = Determina width do "content".
            *    @width 			= Elemento que contém o texto a ser modificado.
            *		 @exemple 		= widthContent('70%');
            */
            function widthContent(width){
            		$('.content').css({
            				'width' : width.toString(),
            				'margin' : '0 auto'
            		});
            }

            /*
            *    Função @setaBlocos
            *    @description = Adiciona blocos esquerdo e direito.
            *    @blocosArray	= Elementos que receberão os blocos "-esq" e "dir".
            *		 @obs: 				= Separar blocos por ",", adicionar quantos desejar.
            *		 @exemple 		= setaBlocos(['.topo', '.conceitual', ]);
            */
            function setaBlocos(blocosArray){
            		if(blocosArray != false){
            				jQuery.each(blocosArray , function(index, value){
            						$(value.toString()).find('.content div:first').wrap('<div class="bloco-esq">').parent().next().wrap('<div class="bloco-dir">');
            				});
            		}
            }

            /*
            *    Função @excluiBloco
            *    @description = Exclui elementos e seu conteudo.
            *    @blocosArray	= Elementos que serão excluidos.
            *		 @obs: 				= O código do elemento também será removido.
            *		 @exemple 		= excluiBloco(['.topo', '.conceitual', ]);
            */
            function excluiBloco(blocosArray){
            		if(blocosArray != false){
            				jQuery.each(blocosArray , function(index, value){
            						$(value.toString()).remove();
            				});
            		}
            }

            /*
            *    Função @maskValor
            *    @description = Cria máscara de valor personalizada.
            *    @input				= Elemento que receberá a máscara.
            *    @moeda				= Símbolo da moeda.
            *    @placeHold 	= Placeholde que atuará no elemento.
            *    @separador  	= Símbolo para separar centenas e inteiros.
            *		 @obs: 				= true => 1.00,00 ; false => 1,000.00
            *		 @exemple 		= maskValor('.valor-entrada', '$', 'Valor da entrada:', true);
            */
            function maskValor(input, moeda, placeHold, separador){
                $j = jQuery.noConflict();
                var obj = ($j(input) === undefined ? $j(input) : $j(input));

                obj.focus(function(event) {
            			if(separador == true){
            		    obj.priceFormat({
            		                prefix: moeda,
            		                centsSeparator: '.',
            		                thousandsSeparator: ','
            		    });
            			}else{
            				obj.priceFormat({
            		                prefix: moeda,
            		                centsSeparator: ',',
            		                thousandsSeparator: '.'
            		    });
            			}
                });

                obj.blur(function(event) {

                    if (obj.val() == (moeda + " 0,00")) {
                        obj.val(placeHold);
                    }
                });
            }

            /*
            *    Função @maps
            *    @description = Cria mapas e seus links de acesso.
            *    @mapsArray		= Iframe's dos mapas.
            *		 @linkMap			= Classe do elemento que ao clicado exibirá o mapa.
            *		 @obs: 				= Deixar os mapas em ordem dos links. É interessante colocar todos os mapas numa variavel e chamá-la na função.
            *		 @exemple 		= maps([
            												'<iframe>mapa1</iframe>',
            												'<iframe>mapa2</iframe>'
            										], '.verunidade');
            */
            function maps(mapsArray, linkMap){
            		var mapPos = 1;
            		jQuery.each(mapsArray , function(index, value){
            				$('.bloco-mapas').append('<div class="map" rel="' + mapPos + '">' + value + '</div>');
            				mapPos += 1;
            		});

            		var vermaisPos = 1;
            		// define em que link clicar //
            		$(linkMap).each(function () {
            				$(this).wrap('<a href="#" rel="' + vermaisPos + '" class="mapLink">');
            				vermaisPos += 1;
            		});

            		$('.mapLink').click(function (event) {
            				event.preventDefault();
            				var mapLinkPos = $(this).attr('rel');
            				$('.map').hide().parent().find(".map[rel=" + mapLinkPos + "]").show();
            		});
            }

            /*
            *    Função @setFancybox
            *    @description = Exibe imagens em fancyBox.
            *    @classImgs		= Classe das imagens.
            *    @altimgs			= Título que aparecerá abaixo da imagem.
            *		 @exemple 		= setFancybox('.imgDetalhe', $('.produto_modelo').first().text());
            */
            function setFancybox(classImgs, altImgs){
            	$j = jQuery.noConflict();
            	$(".fancybox").fancybox({
            				width: 300,
            				height: 300
            		});

            	$j(classImgs).each(function(){
            				var ulrImg = $(this).attr('src');
            				$j(this).wrap('<a href="' + ulrImg + '" class="fancybox" rel="group" alt="' + altImgs.toString() +'" title="' + altImgs.toString() +'">');
            		});
            }


            /*
            *    Função @hover
            *    @description = Hover nos produtos.
            *    @classe		  = Classe dos produtos.
            *		 @exemple 		= hover('.produto');
            */
            function hover(classe){
            	$(classe).hover(function(){
            		$(this).stop().fadeTo( 300 , 0.6)
            	}, function(){
            		$(this).stop().fadeTo( 300 , 1)
            	});
            }

            /*
            *    Função @nomeCamposForm
            *    @description = Deixar o valor do atributo "name" fora do campo.
            *    @campos		  = Classe dos inputs desejados.
            *		 @exemple 		= nomeCamposForm('input');
            */
            function nomeCamposForm(campos){
            	jQuery.each(campos , function(index, value){
            		$(value.toString()).each(function(){
            			var name = $(this).attr('name');
            			var type = $(this).attr('type');

            			if(type == 'hidden'){

            			}else if(type == 'submit'){

            			}else{
            			$(this).before('<p class="nomeCampoForm">'+name+'</p>');
            		}
            		});
            	});
            }

            /*
            *    Função @topoFix
            *    @description = Menu fixo no topo da mobile.
            *    @classFix		= Classe do elemento que ficará fixo.
            *		 @exemple 		= topoFix('.menuFixo');
            */
            function topoFix(classFix){
            		var nav = $(classFix);
            		$(classFix).css('display', 'none');

                $(window).scroll(function () {
                    if ($(this).scrollTop() > 136) {
                        nav.addClass("f-nav");
                    } else {
                        nav.removeClass("f-nav");
                    }
                });
            }

            /*
            *    Função @colunas
            *    @description = Torna os elementos em colunas.
            *    @element		  = Classe do elemento que ficará em colunas.
            *		 @qntColunas  = Quantidade de colunas desejada.
            *		 @exemple 		= colunas('.produto', 4);
            */
            function colunas(element, qntColunas){
            	$( element ).addClass( 'su-column su-column-size-1-' + qntColunas ).parent().addClass( "su-column su-column-size-1-1" ).wrap( '<div class="su-row" >');
            }

            /*
            *    Função @sliceString
            *    @description = Substitui texto usando idenficador como referência.
            *    @element 		= Elemento que contém o texto a ser modificado.
            *    @ident 			= Identificador para "cortar o texto"
            *    @posic 			= (true, false)   true = retorna texto após o identificador
            *                            				false = retorna texto antes do identificador
            *		 @exemple 		= sliceString('select[name=entrada] option', '&', false);
            */
            function sliceString(element, ident, posic){
            	$(element).each(function () {
            		str_val = $(this).text();
            		str_pos = str_val.indexOf(ident);

            		if (str_pos !== -1) {
            			if(posic == true){
            				str_view = str_val.slice(str_pos + 2);
            			}else{
            				str_view = str_val.slice(0 , str_pos);
            			}
            			$(this).text(str_view);
            		}
            	});
            }

            /*
            *    Função @font
            *    @description  = Define font padrão pra LP.
            *    @fontFamily	 = Como será chamada no css.
            *    @urlGoogle 	 = Link do google fonts.
            *    @obs 				 = Inserir apenas o valor do href na urlGoogle. Não esquecer de estilizar a font no css dos elementos.
            *		 @exemple 		 = font('Lato', 'http://fonts.googleapis.com/css?family=Lato:300,400,700');
            */
            function font(fontFamily, urlGoogle){
            	$('body').prepend('<style>*{font-family: ' + fontFamily + ' !important; }</style>');
            	$('head').append("<link href='" + urlGoogle + "' rel='stylesheet' type='text/css'>");
            }
            </script>

            <script>

            		$(document).ready(function () {
